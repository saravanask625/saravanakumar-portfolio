{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
  "saravanakumar-portfolio": {
    "projectType": "application",
    "root": "",
    "sourceRoot": "src",
    "architect": {
      "build": {
        "builder": "@angular-devkit/build-angular:browser",
        "options": {
          "outputPath": "dist/saravanakumar-portfolio",
          "index": "src/index.html",
          "main": "src/main.ts",
          "polyfills": ["src/polyfills.ts"],
          "tsConfig": "tsconfig.json",
          "assets": ["src/assets"],
          "styles": ["src/styles.scss"]
        },
        "configurations": {
          "production": {
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false
          }
        }
      },
      "serve": {
        "builder": "@angular-devkit/build-angular:dev-server",
        "options": {
          "browserTarget": "saravanakumar-portfolio:build"
        },
        "configurations": {
          "production": {
            "browserTarget": "saravanakumar-portfolio:build:production"
          }
        }
      }
    }
  }
},
  "defaultProject": "saravanakumar-portfolio",
  "cli": {
    "analytics": false
  }
}